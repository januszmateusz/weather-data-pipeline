{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current Module",
      "type": "debugpy",
      "request": "launch",
      "module": "src.transformers.weather_transformer",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}